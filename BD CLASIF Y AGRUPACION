****CREACION TABLAS***XML Code**********

<?xml version="1.0" encoding="UTF-8"?>
<mercado>
  <industrias>
    <idindustria primaria="si">entero</idindustria>
    <nbindustria>texto</nbindustria>
  </industrias>

  <empresas>
    <idempresa primaria="si">entero</idempresa>
    <nbempresa>texto</nbempresa>
    <idindustria foranea="si">entero</idindustria>
    <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </empresas>

<encadenamientos>
    <idindustriacompradora primaria="si">entero</idindustriacompradora>
    <idindustriavendedora primaria="si">entero</idindustriavendedora>
    <coeficiente>entero</coeficiente>
  </encadenamientos>

  <solicitudes>
    <idsolicitud primaria="si">entero</idsolicitud>
    <idindustria foranea="si">entero</idindustria>
    <fecentregasol>fecha</fecentregasol>
    <idindustriacompradora foranea="si">entero</idindustriacompradora>
    <cantsolicitada>entero</cantsolicitada>
    <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </solicitudes>

  <nivelesvariables>
    <idempresa primaria="si">entero</idempresa>
    <idalmacen primaria="si">entero</idalmacen>
    <deseado>entero</deseado>
    <actual>entero</actual>
    <minimodeseado>entero</minimodeseado>
    <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </nivelesvariables>

<ultimasincronizacion>
<id>entero</id>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</ultimasincronizacion>

  <pagos>
    <idpago primaria="si">entero</idpago>
    <idoperacion foranea="si">entero</idoperacion>
    <cantidadpagada>decimal</cantidadpagada>
    <fecpago>fecha</fecpago>
    <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </pagos>

  <reglascompensacion>
    <idregla primaria="si">entero</idregla>
    <nbregla>texto</nbregla>
  </reglascompensacion>

  <empresaspenalizadas>
    <idempresa primaria="si">entero</idempresa>
    <idregla primaria="si">entero</idregla>
    <idempresavictima primaria="si">entero</idempresavictima>
    <notificado>entero</notificado>
    <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </empresaspenalizadas>

  <almacenes>
   <idalmacen primaria="si">entero</idalmacen>
   <idregla primaria="si">entero</idregla>
   <nbalmacen>texto</nbalmacen>
   <idtipoalmacen>entero</idtipoalmacen>
   <maxalmacen>entero</maxalmacen>
   <fecha_sincronizacion>fecha</fecha_sincronizacion>
  </almacenes>

<balances>
<idempresa primaria="si">entero</idempresa>
<fecbalance>fecha</fecbalance>
<idcuenta>entero</idcuenta>
<saldo>decimal</saldo>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</balances>

<usuarios>
<idusuario primaria="si">entero</idusuario>
<noboleta>texto</noboleta>
<contrasena>texto</contrasena>
<nombre>texto</nombre>
<apepaterno>texto</apepaterno>
<apematerno>texto</apematerno>
<grupo>texto</grupo>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</usuarios>

<compras>
<idcompra primaria="si">entero</idcompra>
<idcotizacion>texto</idcotizacion>
<feccompra>fecha</feccompra>
<liquidada>entero</liquidada>
<entregada>entero</entregada>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</compras>

<comprasoperaciones>
<idoperacion primaria="si">entero</idoperacion>
<idtipooperacion>entero</idtipooperacion>
<idcompra>entero</idcompra>
<idempresacompradora>entero</idempresacompradora>
<idempresavendedora>entero</idempresavendedora>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</comprasoperaciones>

<embarques>
<idembarques primaria="si">entero</idembarques>
<idoperacion>entero</idoperacion>
<cantidadembarcada>entero</cantidadembarcada>
<fecembarque>fecha</fecembarque>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</embarques>

<usuariosempresas>
<idusuario>entero</idusuario>
<idempresa>entero</idempresa>
</usuariosempresas>

<cotizaciones>
<idcotizacion primaria="si">entero</idcotizacion>
<idsolicitud>entero</idsolicitud>
<cantofrecida>entero</cantofrecida>
<precio>decimal</precio>
<fecexpiracion>fecha</fecexpiracion>
<fecentrega>fecha</fecentrega>
<estado>entero</estado>
<idempresavendedora>entero</idempresavendedora>
<fecha_sincronizacion>fecha</fecha_sincronizacion>
</cotizaciones>

<cuentas>
<idcuenta primaria="si">entero</idcuenta>
<nbcuenta>texto</nbcuenta>
<acreedora>entero</acreedora>
</cuentas>

<tipooperaciones>
<idtipooperaciones primaria="si">entero</idtipooperaciones>
<nbtipooperaciones>texto</nbtipooperaciones>
</tipooperaciones>

<tipoalmacen>
<idtipoalmacen primaria="si">entero</idtipoalmacen>
<nbtipoalmacen>texto</nbtipoalmacen>
</tipoalmacen>

</mercado>


***************XSLT Code************************************

<?xml version="1.0" encoding="UTF-8"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:template match="*">
   <xsl:for-each select="child::*"> 
      CREATE TABLE <xsl:value-of select="name(.)"/>
        ( <xsl:for-each select="child::*">
          <xsl:value-of select="name(.)"/>
          
         <xsl:choose>
              <xsl:when test=".='entero'"> int</xsl:when>
               <xsl:when test=".='texto'"> text</xsl:when>
               <xsl:when test=".='logico'"> boolean</xsl:when>
               <xsl:when test=".='decimal'"> double</xsl:when>
               <xsl:when test=".='fecha'"> date</xsl:when>

         <xsl:otherwise>
             ninguno
        </xsl:otherwise>      
                
 </xsl:choose>, 
 </xsl:for-each>
    PRIMARY KEY (
<xsl:for-each select="*">
<xsl:if test="@primaria='si'">
<xsl:value-of select="name(.)"/>
</xsl:if>
       <xsl:if test="following-sibling::*/@primaria='si'">, </xsl:if>
</xsl:for-each>
));<br/>
 </xsl:for-each> 
 <xsl:for-each select="instancias/*"> <br/>
Insert into <xsl:value-of select="name()"/> values (
    <xsl:for-each select="*">
'<xsl:value-of select="."/>'
  <xsl:if test="following-sibling::*">, </xsl:if>
</xsl:for-each>
);
</xsl:for-each>
</xsl:template>
</xsl:stylesheet>

******************CREACION DE TABLAS EN SQL***************
CREATE TABLE empresas ( idempresa int, nbempresa text, idindustria int, fecha_sincronizacion date, PRIMARY KEY ( idempresa )); 

CREATE TABLE encadenamientos ( idindustriacompradora int, idindustriavendedora int, coeficiente int); 

CREATE TABLE solicitudes ( idsolicitud int, idindustria int, fecentregasol date, idindustriacompradora int, cantsolicitada int, fecha_sincronizacion date, PRIMARY KEY ( idsolicitud )); 

CREATE TABLE nivelesvariables ( idempresa int, idalmacen int, deseado int, actual int, minimodeseado int, fecha_sincronizacion date, PRIMARY KEY ( idempresa, idalmacen )); 

CREATE TABLE pagos ( idpago int, idoperacion int, cantidadpagada double, fecpago date, fecha_sincronizacion date, PRIMARY KEY ( idpago )); 

CREATE TABLE reglascompensacion ( idregla int, nbregla text, PRIMARY KEY ( idregla )); 

CREATE TABLE empresaspenalizadas ( idempresa int, idregla int, idempresavictima int, notificado int, fecha_sincronizacion date, PRIMARY KEY ( idempresa, idregla, idempresavictima )); 

CREATE TABLE compras ( idcompra int, idcotizacion int, feccompra date, liquidada int, entregada int, fec_sincronizacion date, PRIMARY KEY ( idcompra )); 

CREATE TABLE comprasoperaciones ( idoperacion int, idtipooperacion int, idcompra int, idempresacompradora int, idempresavendedora int, fecha_sincronizacion date, PRIMARY KEY ( idoperacion)); 

CREATE TABLE cotizaciones ( idcotizacion int, idsolicitud int, cantofrecida int, precio float, fecexpiracion date, fecentrega date, estado int, idempresavendedora int, fecha_sincronizacion date, PRIMARY KEY ( idcotizacion )); 

CREATE TABLE embarques ( idembarque int, idoperacion int, cantidadembarcada int, fecembarque date, fecha_sincronizacion date, PRIMARY KEY ( idembarque )); 

CREATE TABLE cuentas ( idcuenta int, nbcuenta text, acreedora int, PRIMARY KEY ( idcuenta )); 

CREATE TABLE tipoalmacen ( idtipoalmacen int, nbtipoalmacen text, PRIMARY KEY ( idtipoalmacen )); 

CREATE TABLE ultimasincronizacion( id int, fecha_sincronizacion date); 

CREATE TABLE usuarioempresas( idusuario int, idempresa int); 

CREATE TABLE tipooperaciones( idtipooperaciones int, nbtipooperaciones text, PRIMARY KEY(idtipooperaciones)); 

CREATE TABLE usuarios ( idusuario int, noboleta text, contrasena text, nombre text, apepaterno text, apematerno text, grupo text, fecha_sincronizacion date, PRIMARY KEY ( idusuario )); 

CREATE TABLE almacenes (idalmacen int, idempresa int, nbalmacen text, idtipoalmacen int, maxalmacen int, fecha_sincronizacion date, PRIMARY KEY ( idalmacen)); 

CREATE TABLE balances (idempresa int, fecbalance date, idcuenta int, saldo double, fecha_sincronizacion date, PRIMARY KEY ( idempresa)); 

CREATE TABLE industrias (idindustria int, nbindustria text, PRIMARY KEY ( idindustria )); 

********************
<?xml version="1.0" encoding="ISO-8859-1"?>
<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform">
<xsl:template match="//mercado">
<xsl:for-each select="child::*"> 
 <xsl:choose>
 <xsl:when test="name()='disenio'"> 
 <xsl:for-each select="child::*">
     CREATE TABLE <xsl:value-of select="name(.)"/>
        ( <xsl:for-each select="child::*">
            <xsl:value-of select="name(.)"/> 
             <xsl:choose>
               <xsl:when test="(.)='entero'"> int</xsl:when>
               <xsl:when test="(.)='texto'"> text</xsl:when>
               <xsl:when test="(.)='logico'"> boolean</xsl:when>
               <xsl:when test="(.)='decimal'"> double</xsl:when>
               <xsl:when test="(.)='fecha'"> date</xsl:when>
              </xsl:choose>
       <xsl:if test="following-sibling::*">, </xsl:if>   
           </xsl:for-each>);<br/>
       </xsl:for-each>
</xsl:when>
<xsl:when test="name()='registros'"> 
 <xsl:for-each select="child::*">
     Insert into <xsl:value-of select="name(.)"/> values (
     <xsl:for-each select="child::*">
       <xsl:value-of select="(.)"/>
       <xsl:if test="following-sibling::*">, </xsl:if>   
     </xsl:for-each>
);<br/>
 </xsl:for-each>
</xsl:when>
</xsl:choose>
</xsl:for-each> <br/>
</xsl:template>
</xsl:stylesheet>

